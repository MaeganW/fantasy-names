{
  "name": "fantasy-names",
  "version": "1.0.0",
  "description": "For all your random fantasy character needs.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel  --copy-files  --out-dir dist  --ignore *.test.js src",
    "lint": "eslint .",
    "test": "nyc mocha src/index.test.js",
    "validate": "npm-run-all --parallel test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaeganW/fantasy-names.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "fantasy",
    "names"
  ],
  "author": "Maegan Womble <maeganwomble@gmail.com> (https://maeganwomble.herokuapp.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaeganW/fantasy-names/issues"
  },
  "homepage": "https://github.com/MaeganW/fantasy-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.2.0",
    "eslint": "5.14.0",
    "eslint-config-google": "0.12.0",
    "ghooks": "2.0.4",
    "mocha": "6.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "13.3.0",
    "rimraf": "2.6.3"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "branches": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}